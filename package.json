{
  "name": "voices-of-the-court",
  "productName": "VOTC",
  "version": "2.0.0-alpha.rc2",
  "description": "Reimplementation of Voices of the Court mod for Crusader Kings III for better experience",
  "main": "out/main/main.js",
  "scripts": {
    "typedefs": "node ./build/createGameDataTypeDefs.js",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev -w --inspect",
    "build": "npm run typedefs && npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac"
  },
  "keywords": [],
  "author": {
    "name": "MrAndroPC",
    "email": "sared3420@gmail.com"
  },
  "license": "GPL-3.0-only",
  "devDependencies": {
    "@electron-forge/cli": "^7.8.0",
    "@electron-forge/maker-deb": "^7.8.0",
    "@electron-forge/maker-rpm": "^7.8.0",
    "@electron-forge/maker-squirrel": "^7.8.0",
    "@electron-forge/maker-zip": "^7.8.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.0",
    "@electron-forge/plugin-fuses": "^7.8.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/fuses": "^1.8.0",
    "@types/handlebars": "^4.1.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.4.1",
    "electron": "35.1.5",
    "electron-builder": "^26.0.12",
    "electron-vite": "^4.0.0",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.31.0",
    "sass": "^1.87.0",
    "ts-node": "^10.9.2",
    "ts-to-jsdoc": "^2.4.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6"
  },
  "dependencies": {
    "@logdna/tail-file": "^4.0.2",
    "active-win": "^8.2.1",
    "archiver": "^7.0.1",
    "electron-log": "^5.4.3",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.7.3",
    "handlebars": "^4.7.8",
    "i18next": "^25.8.4",
    "i18next-browser-languagedetector": "^8.2.0",
    "ollama": "^0.5.15",
    "openai": "^4.98.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^16.5.4",
    "streaming-markdown": "^0.2.15",
    "uuid": "^11.1.0",
    "zod": "^3.23.8",
    "zustand": "^5.0.8"
  }
}
